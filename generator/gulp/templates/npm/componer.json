{
	"name": "{{componout-name}}",
	"version": "0.0.1",
	"type": "npm",
	"build": {
		"from": "src/index.js",
		"to": "dist/index.js",
		"options": {
			"minify": false,
			"sourcemap": false,
			"vendors": false,
			"hashfile": false
		},
		"settings": {
			"output": {
				"libraryTarget": "commonjs2"
			},
			"target": "node",
			"node": {
				"global": false,
				"Buffer": false,
				"__dirname": false,
				"__filename": false
			}
		}
	},
	"test": {
		"entry": {
			"from": "test/specs/index.js",
			"settings": {}
		},
		"browsers": "Terminal"
	}
}
