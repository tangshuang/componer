{
	"entry": {
		"script": "src/script/{{componout-name}}.js",
		"index": "preveiw/index.html",
		"preveiw": "src/script/{{componout-name}}.js",
		"server": "preveiw/server.js",
		"test": "test/specs"
	},
	"output": {
		"script": "dist",
		"reporters": "test/reporters"
	},
	"webpack": {
		"_minify": true,
		"output": {
			"libraryTarget": "umd",
			"filename": "{{componout-name}}.js",
			"library": "{{componoutName}}",
			"sourceMapFilename": "{{componout-name}}.js.map"
		},
		"externals": {},
		"resolve": {
			"alias": {}
		}
	},
	"sass": {
		"_minify": true,
		"output": {
			"filename": "{{componout-name}}.css",
			"sourcemap": true
		}
	},
	"karma": {
		"singleRun": true,
		"browsers": ["PhantomJS"]
	}
}